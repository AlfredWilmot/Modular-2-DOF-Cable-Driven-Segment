<!-- All launch files needed in ordre to do some basic motion palnning  -->
<launch>

    <!-- launch robot sim -->
    <include file="$(find sara_8dof_v1)/launch/segment_sim.launch"/>

    <!-- launch moveit using the configuration generated by the wizard, for path planning and accepting goals -->
    <include file="$(find sara_moveit_config)/launch/move_group.launch"/>
    
    <!-- <node pkg="moveit_ros_move_group" type="move_group" name="remap_to_SARA" output="screen">
    <remap from="joint_states" to="SARA/joint_states"/>
    </node> -->


    <!-- use rviz to send configuration to send goals -->
    <include file="$(find sara_moveit_config)/launch/moveit_rviz.launch">
        <arg name="config" value="true"/>
    </include>
    
</launch>